public class Counter {
    public static int count;

    public Counter() {

       count = count + 1;
    }

    public static void main(String[] args)
    {
        Counter C1 = new Counter();
        Counter C2 = new Counter();
        Counter C3 = new Counter();
        System.out.println(C1.count);
    }
}

